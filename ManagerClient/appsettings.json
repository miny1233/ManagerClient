{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Kestrel": {
    "Endpoints": {
      "HttpsDefaultCert": {
          "Url": "https://*:443",
          "Protocols": "Http1AndHttp2AndHttp3",
          "Certificate" :{
            "Path": "./cert.pem",
            "KeyPath": "./cert.key"
          }
        }
    }
  }
}

